<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "urlrewrite3.2.dtd">
<urlrewrite>
	<rule>
		<from>/(news).html</from>
		<to>/app/$1/$1.jsp</to>
	</rule>
	<rule>
		<from>/news/(\d+).html</from>
		<to>/app/news/news.jsp?catalog=$1</to>
	</rule>
	<rule>
		<from>/news/p/(\d+).html</from>
		<to>/app/news/news.jsp?pageNumber=$1&amp;p=true</to>
	</rule>
</urlrewrite>

