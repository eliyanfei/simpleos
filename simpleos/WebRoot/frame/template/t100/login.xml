<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.simpleframework.net/xsd/default/simple.xsd">
	<scriptEval>single</scriptEval>
	<handleClass>net.prj.manager.template.PrjTemplatePageHandle</handleClass>
	<jobView>${net.simpleframework.organization.IJob.sj_account_normal}</jobView>
	<components>
		<login name="__default_login" containerId="__default_login"
			showResetAction="true" showGetPassword="true"
			registAction="$Actions.loc('/register.html');" showAutoLogin="true">
			<loginCallback>
				<![CDATA[
					if(json['ok'])
						$Actions.loc('/index.html');
				]]>
			</loginCallback>
		</login>
	</components>
</page>