<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.simpleframework.net/xsd/default/simple.xsd">
	<scriptEval>single</scriptEval>
	<importPage>
		<value>/simple/template/c.xml</value>
		<value>/app/docu/docu_bar.xml</value>
		<value>mydocu_list.xml</value>
	</importPage>
	<handleClass>net.prj.manager.template.PrjTemplatePageHandle</handleClass>
	<jobView>sys_account_normal</jobView>
	<components>
		<ajaxRequest name="myDocuTableAct" updateContainerId="mydocuId"
			includeRequestData="p">
			<urlForward>mydocu_list.jsp</urlForward>
		</ajaxRequest>
		<ajaxRequest name="myUploadAct" updateContainerId="mydocuId">
			<urlForward>upload.jsp</urlForward>
		</ajaxRequest>
		<ajaxRequest name="myDownloadAct" updateContainerId="mydocuId">
			<urlForward>down_list.jsp</urlForward>
		</ajaxRequest>
	</components>
</page>