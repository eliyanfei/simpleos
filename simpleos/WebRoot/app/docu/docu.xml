<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.simpleframework.net/xsd/default/simple.xsd">
	<scriptEval>single</scriptEval>
	<handleClass>net.simpleos.backend.template.TemplatePageHandle
	</handleClass>
	<jobView>${net.simpleframework.organization.IJob.sj_anonymous}
	</jobView>
	<components>
		<tree name="docuTreeAct" containerId="docu_tree"
			handleClass="net.itsite.docu.DocuCatalogTreeHandle"></tree>
		<ajaxRequest name="doUploadAct" handleClass="net.itsite.docu.DocuAjaxHandle"
			handleMethod="doUpload">
			<jsCompleteCallback>
				<![CDATA[
					$Actions.loc('/docu/upload.html');
				]]>
			</jsCompleteCallback>
		</ajaxRequest>
		<pager name="documentPaperAct" containerId="documentDivId"
			pagerBarLayout="bottom" pageItems="49" showEditPageItems="false"
			noResultDesc="no document" dataPath="docu_data.jsp" handleClass="net.itsite.docu.DocuPaperHandle">
		</pager>
	</components>
</page>